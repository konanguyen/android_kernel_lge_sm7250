#include <dt-bindings/clock/qcom,camcc-lagoon.h>

&pm6150l_flash0 {
	status = "ok";
	qcom,max-current = <750>;
	qcom,current-ma = <750>;
	qcom,duration-ms = <500>;
};

&pm6150l_flash1 {
	status = "ok";
	qcom,max-current = <750>;
	qcom,current-ma = <750>;
	qcom,duration-ms = <500>;
};

&pm6150l_flash2 {
	status = "disabled";
};

&pm6150l_torch0 {
	status = "ok";
	qcom,max-current = <150>;
	qcom,current-ma = <150>;
};

&pm6150l_torch1 {
	status = "ok";
	qcom,max-current = <150>;
	qcom,current-ma = <150>;
};

&pm6150l_torch2 {
	status = "disabled";
};

&pm6150l_switch0 {
	status = "ok";
};

&pm6150l_switch1 {
	status = "disbled";
};

