//bug 547982, baoshulin@wingtech.com, 20200418, modify audio bringup smartpa
&qupv3_se7_i2c {
	status = "ok";
	#address-cells = <1>;
	#size-cells = <0>;
	aw882xx_smartpa@34 {
		compatible = "awinic,aw882xx_smartpa_r";
		reg = <0x34>;
		reset-gpio = <&tlmm 102 0>;
		irq-gpio = <&tlmm 99 0x2008>;
		monitor-flag = <1>;
		monitor-timer-val = <3000>;
		status = "okay";
		aw-cali-mode = "aw_misc";
		sound-channel = "right";
	};
	aw882xx_smartpa@36 {
		compatible = "awinic,aw882xx_smartpa_l";
		reg = <0x36>;
		reset-gpio = <&tlmm 101 0>;
		irq-gpio = <&tlmm 100 0x2008>;
		monitor-flag = <1>;
		monitor-timer-val = <3000>;
		status = "okay";
		aw-cali-mode = "aw_misc";
		sound-channel = "left";
	};
};

&tlmm {
	mi2s_2_sck_active: mi2s_2_sck_active {
		mux {
			pins = "gpio72";
			function = "mi2s_2";
		};

		config {
			pins = "gpio72";
			drive-strength = <8>;   /* 8 mA */
			bias-disable;
			output-high;
		};
	};

	mi2s_2_sck_sleep: mi2s_2_sck_sleep {
		mux {
			pins = "gpio72";
			function = "mi2s_2";
		};

		config {
			pins = "gpio72";
			drive-strength = <2>;   /* 2 mA */
			bias-disable;
			output-high;
		};
	};

	mi2s_2_ws_active: mi2s_2_ws_active {
		mux {
			pins = "gpio74";
			function = "mi2s_2";
		};

		config {
			pins = "gpio74";
