			output-high;
		};
	};

	mi2s_2_data0_active: mi2s_2_data0_active {
		mux {
			pins = "gpio73";
			function = "mi2s_2";
		};

		config {
			pins = "gpio73";
			drive-strength = <8>;   /* 8 mA */
			bias-disable;
			output-high;
		};
	};

	mi2s_2_data0_sleep: mi2s_2_data0_sleep {
		mux {
			pins = "gpio73";
			function = "mi2s_2";
		};

		config {
			pins = "gpio73";
			drive-strength = <2>;   /* 8 mA */
			bias-disable;
			output-high;
		};
	};

	mi2s_2_data1_active: mi2s_2_data1_active {
		mux {
			pins = "gpio60";
			function = "mi2s_2";
		};

		config {
			pins = "gpio60";
			drive-strength = <8>;   /* 8 mA */
			bias-disable;
			output-high;
		};
	};

